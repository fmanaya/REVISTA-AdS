<html>
	<head>
		<meta charset="utf-8">
		<title><%= @document.title %> | <%= @site.title %></title>
		<%- @getBlock("meta").toHTML() %>
		<%- @getBlock("styles").add(["/styles/style.css"]).toHTML() %>
		<%- @getBlock("scripts").add(["/vendor/jquery.js", "/vendor/jquery.collagePlus.min.js", "/scripts/script.js"]).toHTML() %>
	</head>
	<body>	

		<h1 class="nrorev">170</h1>
		<ul>
			<% for page in @getSorted(170).toJSON(): %>
				<li class="<%= if page.id is @document.id then 'active' else 'inactive' %>">
					<a href="<%= page.url %>">
						<%= page.title %>
					</a>
				</li>
			<% end %>
		</ul>	
		<h1 class="nrorev">171</h1>
		<ul>
			<% for page in @getSorted(171).toJSON(): %>
				<li class="<%= if page.id is @document.id then 'active' else 'inactive' %>">
					<a href="<%= page.url %>">
						<%= page.title %>
					</a>
				</li>
			<% end %>
		</ul>
